    <script>
        // Estado global de la aplicaci√≥n
        let selectedServices = [];
        let categories = [];

        // Funci√≥n para agregar una nueva categor√≠a
        function addCategory(categoryName, services) {
            const category = {
                id: categories.length + 1,
                name: categoryName,
                services: services
            };
            categories.push(category);
            renderCategories();
        }

        // Renderizar todas las categor√≠as
        function renderCategories() {
            const container = document.getElementById('categories-container');
            
            if (categories.length === 0) {
                container.innerHTML = `
                    <div class="text-center py-12 text-gray-500">
                        <svg class="mx-auto h-16 w-16 text-gray-300 mb-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6v6m0 0v6m0-6h6m-6 0H6" />
                        </svg>
                        <h3 class="text-lg font-medium mb-2">Esperando categor√≠as...</h3>
                        <p>Comparte la primera categor√≠a con sus precios para comenzar</p>
                    </div>
                `;
                return;
            }

            container.innerHTML = categories.map(category => `
                <div class="border border-gray-200 rounded-lg p-4">
                    <h4 class="text-lg font-semibold text-gray-800 mb-3">${category.name}</h4>
                    <div class="grid gap-3">
                        ${category.services.map(service => `
                            <div class="price-card bg-gray-50 hover:bg-purple-50 border border-gray-200 hover:border-purple-300 rounded-lg p-4 cursor-pointer transition-all" onclick="toggleService('${category.id}', '${service.name}', ${service.price})">
                                <div class="flex justify-between items-start mb-2">
                                    <div class="flex items-start flex-1">
                                        <input type="checkbox" id="service-${category.id}-${service.name}" class="mr-3 mt-1 text-purple-600 rounded" ${isServiceSelected(category.id, service.name) ? 'checked' : ''}>
                                        <div class="flex-1">
                                            <span class="font-medium text-gray-800 block">${service.name}</span>
                                            ${service.details ? `<p class="text-sm text-gray-600 mt-1">${service.details}</p>` : ''}
                                        </div>
                                    </div>
                                    <span class="text-purple-600 font-bold text-lg ml-4">$${service.price.toLocaleString()}</span>
                                </div>
                            </div>
                        `).join('')}
                    </div>
                </div>
            `).join('');
        }

        // Verificar si un servicio est√° seleccionado
        function isServiceSelected(categoryId, serviceName) {
            return selectedServices.some(s => s.categoryId == categoryId && s.name === serviceName);
        }

        // Toggle servicio
        function toggleService(categoryId, serviceName, price) {
            const existingIndex = selectedServices.findIndex(s => s.categoryId == categoryId && s.name === serviceName);
            
            if (existingIndex > -1) {
                selectedServices.splice(existingIndex, 1);
            } else {
                selectedServices.push({
                    categoryId: categoryId,
                    name: serviceName,
                    price: price
                });
            }
            
            updateQuote();
            renderCategories(); // Re-render para actualizar checkboxes
        }

        // Actualizar cotizaci√≥n
        function updateQuote() {
            const servicesContainer = document.getElementById('selected-services');
            const totalElement = document.getElementById('total-price');
            
            if (selectedServices.length === 0) {
                servicesContainer.innerHTML = `
                    <div class="text-center py-8 text-gray-400">
                        <svg class="mx-auto h-12 w-12 text-gray-300 mb-3" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                            <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v10a2 2 0 002 2h8a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2" />
                        </svg>
                        <p class="text-sm">Selecciona servicios para ver tu cotizaci√≥n</p>
                    </div>
                `;
                totalElement.textContent = '$0';
                return;
            }

            servicesContainer.innerHTML = selectedServices.map(service => `
                <div class="flex justify-between items-center bg-purple-50 p-3 rounded-lg">
                    <span class="text-sm font-medium text-gray-700">${service.name}</span>
                    <div class="flex items-center">
                        <span class="text-purple-600 font-bold mr-2">$${service.price.toLocaleString()}</span>
                        <button onclick="removeService('${service.categoryId}', '${service.name}')" class="text-red-500 hover:text-red-700">
                            <svg class="h-4 w-4" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                                <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M6 18L18 6M6 6l12 12" />
                            </svg>
                        </button>
                    </div>
                </div>
            `).join('');

            const total = selectedServices.reduce((sum, service) => sum + service.price, 0);
            totalElement.textContent = `$${total.toLocaleString()}`;
        }

        // Remover servicio espec√≠fico
        function removeService(categoryId, serviceName) {
            selectedServices = selectedServices.filter(s => !(s.categoryId == categoryId && s.name === serviceName));
            updateQuote();
            renderCategories();
        }

        // Limpiar todos los servicios
        function clearAllServices() {
            selectedServices = [];
            updateQuote();
            renderCategories();
        }

        // Descargar cotizaci√≥n
        function downloadQuote() {
            if (selectedServices.length === 0) {
                alert('Selecciona al menos un servicio para generar la cotizaci√≥n');
                return;
            }

            const total = selectedServices.reduce((sum, service) => sum + service.price, 0);
            const date = new Date().toLocaleDateString('es-ES');
            
            let content = `COTIZACI√ìN - IMPULSIVO.MX\n`;
            content += `Fecha: ${date}\n\n`;
            content += `SERVICIOS SELECCIONADOS:\n`;
            content += `${'='.repeat(40)}\n`;
            
            selectedServices.forEach(service => {
                content += `${service.name.padEnd(25)} $${service.price.toLocaleString().padStart(10)}\n`;
            });
            
            content += `${'='.repeat(40)}\n`;
            content += `TOTAL: $${total.toLocaleString()}\n\n`;
            content += `¬°Gracias por confiar en IMPULSIVO.MX!\n`;
            content += `Instagram: @impulsivo.mx\n`;
            content += `Contacto: [Tu informaci√≥n de contacto]`;

            const blob = new Blob([content], { type: 'text/plain' });
            const url = window.URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = `Cotizacion_IMPULSIVO_${date.replace(/\//g, '-')}.txt`;
            document.body.appendChild(a);
            a.click();
            document.body.removeChild(a);
            window.URL.revokeObjectURL(url);
        }

        // Inicializar la aplicaci√≥n
        document.addEventListener('DOMContentLoaded', function() {
            // Agregar la primera categor√≠a: Fotograf√≠a de Retrato
            addCategory('üì∑ Fotograf√≠a de Retrato', [
