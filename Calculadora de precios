                { name: 'Bodas (cobertura premium)', price: 24000, details: '2 fot√≥grafos, cobertura todo el d√≠a, √°lbum impreso, sesi√≥n previa' },
                { name: 'Cumplea√±os infantiles', price: 5000, details: '2-3 horas, fotos del evento, pastel, invitados, entrega digital' },
                { name: 'Quincea√±eras', price: 9500, details: '4-6 horas, sesi√≥n previa, ceremonia, fiesta, √°lbum opcional' },
                { name: 'Aniversarios', price: 6000, details: '3-5 horas, pareja + invitados, cobertura emocional y social' },
                { name: 'Graduaciones (evento grupal)', price: 5250, details: '2-4 horas, entrega por alumno (extra), fotos grupales e individuales' },
                { name: 'Eventos corporativos', price: 7500, details: '2-5 horas, branding, retratos, fotos del ambiente y networking' }
            ]);

            // Agregar tercera categor√≠a: Fotograf√≠a Comercial / Publicitaria
            addCategory('üíº Fotograf√≠a Comercial / Publicitaria', [
                { name: 'Productos e-commerce / cat√°logos', price: 3500, details: 'Hasta 10 productos, fondo blanco, edici√≥n b√°sica, entrega digital' },
