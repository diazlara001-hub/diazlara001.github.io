            addCategory('üñºÔ∏è Servicios de Impresi√≥n y Entrega', [
                { name: '√Ålbum fotogr√°fico personalizado', price: 2100, details: '20-40 p√°ginas, portada r√≠gida, dise√±o incluido' },
